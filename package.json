{
  "name": "islamic-quiz-card-game",
  "version": "1.0.0",
  "description": "Multiplayer Islamic Quiz Card Game built with HTML5/JavaScript",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "webpack --mode production",
    "dev-frontend": "webpack serve --mode development",
    "test": "jest",
    "setup": "node setup.js",
    "mobile:setup": "cd mobile && npm install && npm run setup",
    "mobile:build": "npm run build && cd mobile && npm run build",
    "mobile:build:android": "npm run build && cd mobile && npm run build:android",
    "mobile:build:ios": "npm run build && cd mobile && npm run build:ios",
    "mobile:run:android": "cd mobile && npm run run:android",
    "mobile:run:ios": "cd mobile && npm run run:ios",
    "mobile:clean": "cd mobile && npm run clean",
    "install:all": "npm install && cd mobile && npm install",
    "docker:build": "docker build -t islamic-quiz .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "postinstall": "echo 'Run npm run setup to initialize the project'"
  },
  "keywords": [
    "islamic",
    "quiz",
    "card-game",
    "multiplayer",
    "html5",
    "phaser"
  ],
  "author": "Ummah360",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "mongoose": "^8.0.3",
    "redis": "^4.6.12",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "phaser": "^3.70.0",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.6.0",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "file-loader": "^6.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.7.0"
  }
}